        "react": "18.3.1",
        "react-beautiful-dnd": "13.1.1",
        "react-chartjs-2": "5.2.0",
        "react-dom": "18.3.1",
        "react-hotkeys-hook": "4.5.0",
        "react-hot-toast": "2.4.1",
